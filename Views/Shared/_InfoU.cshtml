@for (int i = 0; i < Model.commodel3.Count(); i++)
{
    <p>@Model.commodel3[i].area</p>
    for (int j = 0; j < Model.model3.Count(); j++)
    {
        if (Model.commodel3[i].idarea == Model.model3[j].idarea)
        {
            <p>@Model.model3[j].carrera</p>
        }
    }
}
